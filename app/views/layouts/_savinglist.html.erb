<% savings.each do |saving| %>
  <%= link_to saving do %></td>
  <div class="object-display">
    <img class="object-image" src=<%= get_group_icon(saving) %> alt="" />
    <div class="object-info">
      <h3><%= saving.name %></h3>
      Created by: <%= saving.author.name %><br>
      <%= saving.created_at.strftime('%e-%b-%Y') %>
    </div>
    <div class="object-amount">
      $<%= ActiveSupport::NumberHelper.number_to_delimited(saving.amount) %>
    </div>
  </div>
  <% end %>
<% end %>